{
  "root": true,
  "globals": {
    "$t": "readonly"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-continue": 0,
    "no-restricted-globals": 0,
    "implicit-arrow-linebreak": 0,
    "prefer-rest-params": 0,
    "vars-on-top": 0,
    "object-shorthand": 0,
    "one-var-declaration-per-line": 0,
    "arrow-parens": 0
  }
}
